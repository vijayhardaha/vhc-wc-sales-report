{
  "name": "vhc-wc-sales-report",
  "version": "1.0.0",
  "description": "Generates a custom product sales report during a specified time period with a filtering option.",
	"license": "GPLv2+",
  "author": "Vijay Hardaha <https://twitter.com/vijayhardaha>",
  "homepage": "https://github.com/vijayhardaha/vhc-wc-sales-report#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vijayhardaha/vhc-wc-sales-report.git"
  },
  "bugs": {
    "url": "https://github.com/vijayhardaha/vhc-wc-sales-report/issues"
  },
  "browserslist": [
		"extends @wordpress/browserslist-config"
	],
  "scripts": {
		"dev": "gulp watch",
		"prebuild": "npm run fix",
		"build": "gulp build",
		"prebuild:css": "npm run fix:css",
		"build:css": "gulp css",
		"prebuild:js": "npm run fix:js",
		"build:js": "gulp js",
		"build:pot": "wp i18n make-pot . languages/vhc-wc-sales-report.pot",
		"fix:css": "stylelint src/scss --fix",
		"fix:js": "eslint src/js --fix",
		"fix:php": "composer run-script fix",
		"fix": "npm run fix:css && npm run fix:js && npm run fix:php",
		"lint:css": "stylelint src/scss",
		"lint:js": "eslint src/js",
		"lint:php": "composer run-script lint",
		"lint": "npm run lint:css && npm run lint:js && npm run lint:php",
		"prezip": "rm -rf vhc-wc-sales-report.zip",
		"zip": "zip -r vhc-wc-sales-report.zip ./*.php includes assets languages"
	},
  "devDependencies": {
		"@wordpress/browserslist-config": "^5.0.0",
		"@wordpress/eslint-plugin": "^13.0.0",
		"@wordpress/prettier-config": "^2.0.0",
		"@wordpress/stylelint-config": "^21.0.0",
		"autoprefixer": "^10.4.8",
    "datatables": "^1.10.18",
		"del": "^6.1.1",
		"eslint": "^8.23.0",
		"eslint-plugin-jsdoc": "^39.3.6",
		"gulp": "^4.0.2",
		"gulp-clean-css": "^4.3.0",
		"gulp-clone": "^2.0.1",
		"gulp-concat": "^2.6.1",
		"gulp-group-css-media-queries": "^1.2.2",
		"gulp-plumber": "^1.2.1",
		"gulp-postcss": "^9.0.1",
		"gulp-rename": "^2.0.0",
		"gulp-sass": "^5.1.0",
		"gulp-strip-css-comments": "^2.0.0",
		"gulp-terser": "^2.1.0",
		"merge-stream": "^2.0.0",
		"postcss-discard-duplicates": "^5.1.0",
		"sass": "^1.54.8",
		"stylelint": "^14.11.0",
		"stylelint-config-property-sort-order-smacss": "^9.0.0",
		"stylelint-order": "^5.0.0"
	}
}

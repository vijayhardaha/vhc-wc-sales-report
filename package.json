{
	"name": "vhc-wc-sales-report",
	"version": "1.0.2",
	"description": "Generates a custom product sales report during a specified time period with a filtering option.",
	"license": "GPLv2+",
	"author": "Vijay Hardaha <https://twitter.com/vijayhardaha>",
	"homepage": "https://github.com/vijayhardaha/vhc-wc-sales-report#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/vijayhardaha/vhc-wc-sales-report.git"
	},
	"bugs": {
		"url": "https://github.com/vijayhardaha/vhc-wc-sales-report/issues"
	},
	"browserslist": [
		"extends @wordpress/browserslist-config"
	],
	"scripts": {
		"dev": "gulp watch",
		"prebuild": "pnpm run lint-fix",
		"prebuild:css": "pnpm run lint-fix:css",
		"prebuild:js": "pnpm run lint-fix:js",
		"build": "gulp build",
		"build:css": "gulp css",
		"build:js": "gulp js",
		"lint:css": "stylelint src/scss",
		"lint:js": "eslint src/js",
		"lint:php": "composer run-script lint",
		"lint": "pnpm run lint:css && pnpm run lint:js && pnpm run lint:php",
		"lint-fix:css": "stylelint src/scss --fix",
		"lint-fix:js": "eslint src/js --fix",
		"lint-fix:php": "composer run-script lint-fix",
		"lint-fix": "pnpm run lint-fix:css && pnpm run lint-fix:js && pnpm run lint-fix:php",
		"prezip": "rm -rf vhc-wc-sales-report.zip",
		"zip": "zip -r vhc-wc-sales-report.zip ./*.php assets includes languages -x '**/.DS_Store'",
		"makepot": "wp i18n make-pot . --exclude=\"node_modules,vendor\" --slug=vhc-wc-sales-report"
	},
	"devDependencies": {
		"@wordpress/browserslist-config": "^5.33.0",
		"@wordpress/eslint-plugin": "^17.7.0",
		"@wordpress/stylelint-config": "^21.33.0",
		"autoprefixer": "^10.4.17",
		"datatables": "^1.10.18",
		"del": "^7.1.0",
		"eslint": "^8.56.0",
		"eslint-plugin-jsdoc": "^48.0.4",
		"gulp": "^4.0.2",
		"gulp-clean-css": "^4.3.0",
		"gulp-clone": "^2.0.1",
		"gulp-concat": "^2.6.1",
		"gulp-group-css-media-queries": "^1.2.2",
		"gulp-plumber": "^1.2.1",
		"gulp-postcss": "^9.1.0",
		"gulp-rename": "^2.0.0",
		"gulp-sass": "^5.1.0",
		"gulp-strip-css-comments": "^3.0.0",
		"gulp-terser": "^2.1.0",
		"merge-stream": "^2.0.0",
		"postcss-discard-duplicates": "^6.0.1",
		"sass": "^1.70.0",
		"stylelint": "^14.16.1",
		"stylelint-config-property-sort-order-smacss": "^10.0.0",
		"stylelint-order": "^6.0.4"
	}
}
